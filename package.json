{
  "name": "angular2-electron-quick-start",
  "version": "0.0.0",
  "description": "Angular 2 with Electron and Webpack",
  "main": "",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "webpack --config config/webpack.prod.js --progress --profile",
    "package": "node package.js",
    "package-all": "npm run package -- --all",
    "electron": "electron dist",
    "webpack-test": "webpack --config webpack.test.js --progress --profile",
    "test": "karma start",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "start": "npm run build:dev && set NODE_ENV=development&& electron dist",
    "start:prod": "npm run build:prod && set NODE_ENV=production&& electron publish",
    "watch": "nodemon --exec \"npm run start\" --ext js,css --ignore build/ --ignore dist/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simplefatty/Angular2-electron-quick-start.git"
  },
  "author": "simplefatty<github@simplefatty.com>",
