{
    "name": "Eolinker",
    "version": "0.0.1",
    "description": "Eolinker,a profession tool for tesnpm t!",
    "main": "",
    "scripts": {
        "build": "npm run build:dev",
        "build:dev": "set NODE_ENV=development&&webpack --config config/webpack.dev.js --progress --profile",
        "build:prod": "set NODE_ENV=production&&webpack --config config/webpack.prod.js --progress --profile",
        "package": "node package.js",
        "prepackage": "npm run build:prod",
        "electron": "electron dist",
        "prebuild:prod": "rm -rf ./publish",
        "prebuild:dev": "rm -rf ./dist",
        "webpack-test": "webpack --config webpack.test.js --progress --profile",
        "test": "set NODE_ENV=test&&karma start",
        "watch:test": "npm run test -- --auto-watch --no-single-run",
        "server:dev:hmr": "npm run server:dev -- --inline --hot",
        "server:dev": "webpack-dev-server --config config/webpack.browser.js --progress --profile --watch --content-base src/",
        "server:prod": "http-server publish --cors",
        "server": "npm run server:dev",
        "start": "npm run build:dev && electron dist",
        "start:dev": "npm run server:dev",
        "start:prod": " npm run build:prod && electron publish",
        "start:hmr": "npm run server:dev:hmr",
        "watch:electron": "nodemon --exec \"npm run start\" --ext ts,scss,html --ignore dist/ --ignore publish/",
        "watch:browser": ""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/simplefatty/Angular2-electron-quick-start.git"
    },
    "author": "simplefatty<github@simplefatty.com>",
